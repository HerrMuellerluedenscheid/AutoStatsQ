- !autostatsq.config.CatalogConfig
  search_events: false
  use_local_catalog: false
  subset_of_local_catalog: false
  use_local_subsets: true
  subset_fns: {'deep': '/home/gesa/Documents/AlpArray/AutoStatsQ_test/TEST/results/catalog/catalog_Mgr6.5_deep.txt',
  'shallow': '/home/gesa/Documents/AlpArray/AutoStatsQ_test/TEST/results/catalog/catalog_Mgr6.5_shallow.txt'}
  catalog_fn: /home/gesa/Documents/AlpArray/AutoStatsQ_test/TEST/results/catalog/catalog_Mgr6.5.txt
  dist: 165.0
  min_mag: 6.5
  max_mag: 8.5
  tmin_str: '2015-01-01 00:00:00'
  tmax_str: '2018-06-30 00:00:00'
  wedges_width: 10
